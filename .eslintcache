aProd = [...productos];\r\n            copiaProd [indice].quanity = nuevaCant;\r\n            setProductos(copiaProd);\r\n            }else{\r\n            let sumoProd = [item, ...productos];\r\n            setProductos(sumoProd)\r\n        };\r\n\r\n    }\r\n\r\n    function eliminarUnItem (item) {\r\n        // console.log(item)\r\n        // let copiaProdB = [...productos];\r\n        const nuevoArreglo = productos.filter(element => element.producto.id !== item)\r\n        console.log(nuevoArreglo);\r\n        setProductos(nuevoArreglo)\r\n    }\r\n\r\n    function vaciarCarrito () {\r\n        \r\n        return setProductos([])\r\n    }\r\n\r\n    //cantidad de items cartwidget\r\n\r\n    let cantidadCart = productos.map (valor => (valor.quanity)) \r\n    let  total = 0 \r\n     for(let i = 0 ; i<cantidadCart.length; i++)\r\n     {\r\n    total += cantidadCart[i];\r\n    }\r\n// console.log(total);\r\n    \r\n    return (\r\n        \r\n        <CartContext.Provider value={{\r\n            \r\n            productos, \r\n            addCart, \r\n            eliminarUnItem,\r\n            vaciarCarrito,\r\n            total\r\n\r\n             }}>\r\n                \r\n            {children}\r\n        </CartContext.Provider>\r\n\r\n    )\r\n}\r\n\r\nexport default CartContextProvider\r\n","C:\\Users\\Santi\\proyectosanti\\src\\components\\Cart\\Cart.jsx",[],"C:\\Users\\Santi\\proyectosanti\\src\\Firebase\\index.jsx",[],"C:\\Users\\Santi\\proyectosanti\\src\\components\\Footer\\footer.jsx",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":12,"column":17,"nodeType":"81","endLine":12,"endColumn":41},{"ruleId":"82","severity":1,"message":"83","line":21,"column":4,"nodeType":"84","endLine":21,"endColumn":6,"suggestions":"85"},{"ruleId":"79","severity":1,"message":"80","line":29,"column":17,"nodeType":"81","endLine":29,"endColumn":42},{"ruleId":"86","severity":1,"message":"87","line":21,"column":